{{log this}}
<header>
    <h1>Video Gallery</h1>
    <nav>
        <ul>
            <li><a href = "/">Home</a></li>
            <li><a href = "/postvideo">PostVideo</a></li>
            {{#if isLoggedIn}}
                <li><a href = "/users/profile/{{user.userId}}">Profile</a></li>
                <li><a href = "/users/logout" id="logout-button">Log out</a></li>
            {{else}}
                <li><a href = "/login" id="authenticate-button">Login</a></li>
                <li><a id="c" href="/registration">Registration</a></li>
            {{/if}}
            <li class="search-bar">
                <form class="search-bar" method="GET" action="/posts/search" enctype="application/x-www-form-urlencoded">
                    <input id="search-text" placeholder="Search Posts" class="search-text" name="searchValue" type="text" />
                    <button type="submit" class="search-button fas fa-search" id="search-submit">Search</button>
                </form>
            </li>
        </ul>
    </nav>
</header>
